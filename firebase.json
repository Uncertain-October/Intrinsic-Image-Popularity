{
  "hosting": {
    "public": "public",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    
    "rewrites": [
      {
        "source": "/",
        "run": {
          "serviceId": "iipa",
          "region": "us-central1",
          "pinTag": true
        }
      }
    ]
  },
  "emulators": {
    "hosting": {
      "port": 5014
    },
    "ui": {
      "enabled": true
    }
  }
}
